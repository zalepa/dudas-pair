<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2007 sp2 (http://www.altova.com) by Diane Le (US Patent and Trademark Office) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:pair="urn:us:gov:uspto:pair" targetNamespace="urn:us:gov:uspto:pair" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="PatentApplicationData">
		<xs:annotation>
			<xs:documentation>Root element. List of elements describing patent application data</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:FileHeader"/>
				<xs:element ref="pair:CorrespondenceInfo"/>
				<xs:element ref="pair:Attorneys" minOccurs="0"/>
				<xs:element ref="pair:ApplicationBibliographicData"/>
				<xs:element ref="pair:Inventors" minOccurs="0"/>
				<xs:element ref="pair:Continuity" minOccurs="0"/>
				<xs:element ref="pair:FileContentHistories" minOccurs="0"/>
				<xs:element ref="pair:ForeignPriorities" minOccurs="0"/>
				<xs:element ref="pair:ImageFileWrapperList" minOccurs="0"/>
				<xs:element ref="pair:SupplementalContents" minOccurs="0"/>
				<xs:element ref="pair:WIPOPublications" minOccurs="0"/>
				<xs:element ref="pair:FirstActionPrediction" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FileHeader">
		<xs:annotation>
			<xs:documentation>Element containing the time and date this file was created</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:FileCreationTimeStamp"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PublicViewingIndicator" type="xs:boolean">
		<xs:annotation>
			<xs:documentation>Boolean indicator showing whether the application is available for public viewing, "true" or "false"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FirstActionPrediction">
		<xs:annotation>
			<xs:documentation>Prediction of time-to-first-action after application reaches Status Code 30</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:PredictionTimePeriod" minOccurs="0"/>
				<xs:element ref="pair:GroupArtUnit" minOccurs="0"/>
				<xs:element ref="pair:ApplicationStatusCode" minOccurs="0"/>
				<xs:element ref="pair:StatusDescription" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TelephoneNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation>Registered attorney telephone number using a 12 character string, i.e. "703-412-6457"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="SupplementalContents">
		<xs:annotation>
			<xs:documentation>List of Supplemental Content document types associated with this application</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:SupplementalContent" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SupplementalContent">
		<xs:annotation>
			<xs:documentation>Supplemental Content document type and number count</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:SupplementalContentCategory">
					<xs:annotation>
						<xs:documentation>String containing a description of the Supplemental Content file type, "Sequence Listings", "Megatables", "Search Results", Computer Program Listings (CPL)", or "Other"</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="pair:SupplementalContentQuantity"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SupplementalContentCategory">
		<xs:annotation>
			<xs:documentation>String containing a description of the Supplemental Content file type, "Sequence Listings", "Megatables", "Search Results", Computer Program Listings (CPL)", or "Other"</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Sequence Listings"/>
				<xs:enumeration value="Megatables"/>
				<xs:enumeration value="Search Results"/>
				<xs:enumeration value="Computer Program Listings"/>
				<xs:enumeration value="Other Supplemental Content Items"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ApplicationStatusCode" type="xs:string">
		<xs:annotation>
			<xs:documentation>String with up to 3 digits, i.e. "150" that represent the current application status</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ScoreFileCategory">
		<xs:annotation>
			<xs:documentation>String containing a description of the Supplemental Content file type, "Sequence Listings", "Megatables", "Search Results", Computer Program Listings (CPL)", or "Other"</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="Sequence Listings"/>
				<xs:enumeration value="MegaTables"/>
				<xs:enumeration value="Search Results"/>
				<xs:enumeration value="Computer Program Listings (CPL)"/>
				<xs:enumeration value="Other"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="SupplementalContentQuantity" type="xs:nonNegativeInteger">
		<xs:annotation>
			<xs:documentation>A short integer representing the number of each file type in the Supplemental Content repository</xs:documentation>
			<xs:documentation>The value of '0' or a positive integer is required for each Supplemental Content file type</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="RegistrationNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation>Practitioner registration number consisting of a 5 character string, i.e. "32884"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="EarliestPublicationYear" type="xs:gYear">
		<xs:annotation>
			<xs:documentation>U.S. Pre-Grant publication year</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PublicationSequenceNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation>U.S. Pre-Grant publication sequence number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PublicationKindCode" type="xs:string">
		<xs:annotation>
			<xs:documentation>U.S. Pre-Grant publication 2 digit Kind Code, i.e. "A1"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PublicationDate" type="xs:date">
		<xs:annotation>
			<xs:documentation>U.S. Pre-Grant publication date as a string value, i.e. "1967-03-03"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ForeignPriorityDate" type="xs:date">
		<xs:annotation>
			<xs:documentation>Foreign Priority date stored as a string, i.e. "YYYY-MM-DD"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ForeignPriorityNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation>Foreign Priority patent number stored as a string, i.e. "2000-042831"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PostalCode" type="xs:string">
		<xs:annotation>
			<xs:documentation>String value containing a postal address code i.e. "92614"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PatentSubclass" type="xs:string">
		<xs:annotation>
			<xs:documentation>Up to a 6 digit integer, i.e. "17130". Subclass values are formatted with a period placed after the third character from the left as XXX.XXX.  Subclass values are displayed with all trailing zeros truncated (i.e. Class/Subclass for 678/006.000 should be displayed as 678/006)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PatentClass" type="xs:string">
		<xs:annotation>
			<xs:documentation>A string displayed as a 3 digit integer for the Patent Class concatenated with the Subclass value formatted as 'CLASS/SUBCLASS'</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ParentContinuityList">
		<xs:annotation>
			<xs:documentation>Group of elements decribing a parent application to this application claims priority</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:ParentContinuity" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ParentContinuity" type="pair:ContinuityType">
		<xs:annotation>
			<xs:documentation>Group of data elements which describe a Parent Continuity claim</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ApplicationStatusText" type="xs:string">
		<xs:annotation>
			<xs:documentation>A string describing application status, i.e. "APPLICATION DISPATCHED FROM OIPE"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ApplicationStatusDate" type="xs:date">
		<xs:annotation>
			<xs:documentation>A string describing the application status date, i.e. "YYYY-MM-DD"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ApplicationBibliographicData">
		<xs:annotation>
			<xs:documentation>Group of patent application bibliographic data elements</xs:documentation>
		</xs:annotation>
		<!-- For the Examiner assigned to this application-->
		<!-- For the Examiner -->
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:ApplicationNumber"/>
				<xs:element ref="pair:ApplicationStatusCode" minOccurs="0"/>
				<xs:element ref="pair:ApplicationStatusText" minOccurs="0"/>
				<xs:element ref="pair:ApplicationStatusDate" minOccurs="0"/>
				<xs:element ref="pair:PublicViewingIndicator"/>
				<xs:element ref="pair:EarliestPublicationYear" minOccurs="0"/>
				<xs:element ref="pair:PublicationDate" minOccurs="0"/>
				<xs:element ref="pair:PublicationKindCode" minOccurs="0"/>
				<xs:element ref="pair:PublicationSequenceNumber" minOccurs="0"/>
				<xs:element ref="pair:AttorneyDocketNumber" minOccurs="0"/>
				<xs:element ref="pair:ConfirmationNumber" minOccurs="0"/>
				<xs:element ref="pair:CustomerNumber" minOccurs="0"/>
				<xs:element ref="pair:FilingDate" minOccurs="0"/>
				<xs:element ref="pair:Section371Sub102eDate" minOccurs="0"/>
				<xs:element ref="pair:ApplicationStorageFormat"/>
				<xs:element ref="pair:ExaminerName" minOccurs="0"/>
				<xs:element ref="pair:GroupArtUnit" minOccurs="0"/>
				<xs:element ref="pair:InventionSubjectMatterType"/>
				<xs:element ref="pair:InventionTitle"/>
				<xs:element ref="pair:FileWrapperLocationChangeDate" minOccurs="0"/>
				<xs:element ref="pair:FileWrapperLocationCode" minOccurs="0"/>
				<xs:element ref="pair:PatentClass" minOccurs="0"/>
				<xs:element ref="pair:PatentSubclass" minOccurs="0"/>
				<xs:element ref="pair:PatentNumber" minOccurs="0"/>
				<xs:element ref="pair:IssueDate" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="BeginPageNumber" type="xs:positiveInteger">
		<xs:annotation>
			<xs:documentation>Document page starting number, a required value for each document listed in the IFW dossier</xs:documentation>
			<xs:documentation>Up to a 4 digit integer, i.e. "9999"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PageQuantity" type="xs:string">
		<xs:annotation>
			<xs:documentation>Document page count, a required value for each document listed in the IFW dossier</xs:documentation>
			<xs:documentation>Up to a 4 digit integer, i.e. "9999"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PageOffsetQuantity" type="xs:positiveInteger">
		<xs:annotation>
			<xs:documentation>Document page offset, a required value for each document listed in the IFW dossier.</xs:documentation>
			<xs:documentation>Up to a 4 digit integer, i.e. "9999"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="MailRoomDate" type="xs:string">
		<xs:annotation>
			<xs:documentation>String "YYYY-MM-DD" containing the official date of outgoing correspondence sent to the applicant</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FileWrapperLocationChangeDate" type="xs:date">
		<xs:annotation>
			<xs:documentation>String "YYYY-MM-DD" containing the date of the last file location change</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FileWrapperLocationCode" type="xs:string">
		<xs:annotation>
			<xs:documentation>String describing the file wrapper location, i.e. "FILE REPOSITORY (FRANCONIA)"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FileWrapperDocumentCode" type="xs:string">
		<xs:annotation>
			<xs:documentation>String containing the Image File Wrapper document code</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ImageFileWrapperList">
		<xs:annotation>
			<xs:documentation>List of documents for this application stored in the Image File Wrapper (IFW)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:ImageFileWrapperDocument" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ImageFileWrapperDocument">
		<xs:annotation>
			<xs:documentation>Individual document elements strored in the Image File Wrapper (IFW)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:DocumentCategory"/>
				<xs:element ref="pair:DocumentDescription"/>
				<xs:element ref="pair:FileWrapperDocumentCode"/>
				<xs:element ref="pair:MailRoomDate" minOccurs="0"/>
				<xs:element ref="pair:PageQuantity" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="IssueDate" type="xs:date">
		<xs:annotation>
			<xs:documentation>A string formated as "YYYY-MM-DD" i.e. "1967-03-03"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Inventors">
		<xs:annotation>
			<xs:documentation>List of Inventors associated with this application</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:Inventor" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Inventor">
		<xs:annotation>
			<xs:documentation>Inventor name and address elements</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:InventorName"/>
				<xs:element ref="pair:InventorAddress"/>
				<xs:element ref="pair:InventorContact" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="InventorName" type="pair:PersonNameType"/>
	<xs:element name="InventorAddress" type="pair:AddressType"/>
	<xs:element name="InventorContact" type="pair:ContactType"/>
	<xs:element name="InventionTitle" type="xs:string">
		<xs:annotation>
			<xs:documentation>String describing the Title of Invention, i.e. "ANCHORED ANULUS METHOD"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="InventionSubjectMatterType" type="xs:string">
		<xs:annotation>
			<xs:documentation>String describing the invention subject matter type, i.e. "UTL"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GroupArtUnit" type="xs:string">
		<xs:annotation>
			<xs:documentation>A 4 character string, i.e. "3738" for the Group Art Unit (GAU) number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ForeignPriorities">
		<xs:annotation>
			<xs:documentation>List of all foreign priority claims for this application</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:ForeignPriority" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ForeignPriority">
		<xs:annotation>
			<xs:documentation>Group of elements describing a foreign priority reference</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:Country"/>
				<xs:element ref="pair:ForeignPriorityNumber"/>
				<xs:element ref="pair:ForeignPriorityDate" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PredictionTimePeriod" type="xs:positiveInteger">
		<xs:annotation>
			<xs:documentation>An optional string value containing as estimate for the number of months to the Examiners first action</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FileCreationTimeStamp" type="xs:dateTime">
		<xs:annotation>
			<xs:documentation>File creation date and time String, i.e. "2007-05-15T13:49:00.0"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="FileContentHistories">
		<xs:annotation>
			<xs:documentation>List containing all file history transactions</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:FileContentHistory" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FileContentHistory">
		<xs:annotation>
			<xs:documentation>Group of elements describing a file history transaction</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:TransactionDate"/>
				<xs:element ref="pair:TransactionDescription"/>
				<xs:element ref="pair:StatusNumber" minOccurs="0"/>
				<xs:element ref="pair:StatusDescription" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TransactionDate" type="xs:date">
		<xs:annotation>
			<xs:documentation>FilGAUe history transaction date like "1994-01-12"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TransactionDescription" type="xs:string">
		<xs:annotation>
			<xs:documentation>String describing a file history transaction i.e., "Issue Fee Payment Verified"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="StatusNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation>An optional 3 character string, i.e. '150' describing past status codes associated with a file content history record</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="StatusDescription" type="xs:string">
		<xs:annotation>
			<xs:documentation>A string containing a description of current and past status changes associated with a file content history record</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DocumentDescription" type="xs:string">
		<xs:annotation>
			<xs:documentation>String containing the IFW document description, i.e. "Internal Response to Printer Query from TC"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DocumentCategory" type="xs:string">
		<xs:annotation>
			<xs:documentation>String containing the IFW document category, i.e. "PROSECUTION"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CustomerNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation>A 5 character string, i.e. "20995" or "L0095"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CorrespondenceInfo">
		<xs:annotation>
			<xs:documentation>Customer contact information consisting of Correspondence and Email addresses</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PersonName" type="pair:PersonNameType" minOccurs="0"/>
				<xs:element name="OrganizationName" type="pair:OrganizationNameType" minOccurs="0"/>
				<xs:element ref="pair:CorrespondenceAddress"/>
				<xs:element ref="pair:CorrespondenceContact" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CorrespondenceAddress" type="pair:AddressType">
		<xs:annotation>
			<xs:documentation>Customer Correspondence address elements</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CorrespondenceContact" type="pair:ContactType"/>
	<xs:element name="Continuity">
		<xs:annotation>
			<xs:documentation>List of parent and child Continuity references</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:ParentContinuityList" minOccurs="0"/>
				<xs:element ref="pair:ChildContinuityList" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ConfirmationNumber" type="xs:string">
		<xs:annotation>
			<xs:documentation>A 4 digit character string, i.e. "3390"</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ChildContinuityList">
		<xs:annotation>
			<xs:documentation>List of applications which claim priority to this application</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:ChildContinuity" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ChildContinuity" type="pair:ContinuityType">
		<xs:annotation>
			<xs:documentation>Group of elements decribing a child application which claims priority to this application</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Attorneys">
		<xs:annotation>
			<xs:documentation>List of registered practitioners associated with this application</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:Attorney" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Attorney">
		<xs:annotation>
			<xs:documentation>List of elements containing a practitioner name, registration and telephone number</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:RegistrationNumber"/>
				<xs:element ref="pair:AttorneyName"/>
				<xs:element ref="pair:AttorneyContact" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AttorneyDocketNumber">
		<xs:annotation>
			<xs:documentation>Attorney Docket Number is stored as a user defined 25 character string</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="25"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AttorneyContact" type="pair:ContactType"/>
	<xs:element name="ApplicationStorageFormat">
		<xs:annotation>
			<xs:documentation>IFW file content indicator, "ELE" for all file wrapper document images, and "FWM" for a partial list of document images</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="ELE"/>
				<xs:enumeration value="FWM"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AttorneyName" type="pair:PersonNameType"/>
	<xs:element name="ExaminerName" type="pair:PersonNameType"/>
	<xs:element name="WIPOPublications">
		<xs:annotation>
			<xs:documentation>World Intellectual Property Organazation published International Application</xs:documentation>
			<xs:documentation>?????????List of WIPO publication data elements</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pair:WIPOPublication" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>List of WIPO publication data elements</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="WIPOPublicationNumber">
		<xs:annotation>
			<xs:documentation>WIPO publication number using an 8 digit integer, i.e. "200001603"</xs:documentation>
		</xs:annotation>
